{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/global/*": [
        "src/global/*"
      ],
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/public/*": [
        "public/*"
      ],
    }
  }
}